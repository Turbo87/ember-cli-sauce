{
  "name": "ember-cli-sauce",
  "version": "1.3.1",
  "description": "Cross browser testing for your ember-cli app using SauceLabs.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "start": "ember server",
    "build": "ember build",
    "test": "ember test",
    "test-sauce": "ember sauce --add-to-ci && ember sauce -b 'internet explorer' -p 'Windows 8.1' --protocol=tap --add-to-ci && ember test"
  },
  "bin": "./bin/cli.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/johanneswuerbach/ember-cli-sauce.git"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "Johannes WÃ¼rbach <johannes.wuerbach@googlemail.com>",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.0.2",
    "ember-cli": "0.2.5",
    "ember-cli-app-version": "0.3.3",
    "ember-cli-content-security-policy": "0.4.0",
    "ember-cli-dependency-checker": "^1.0.0",
    "ember-cli-htmlbars": "0.7.6",
    "ember-cli-ic-ajax": "0.1.1",
    "ember-cli-inject-live-reload": "^1.3.0",
    "ember-cli-qunit": "0.3.13",
    "ember-cli-uglify": "^1.0.1",
    "ember-data": "1.0.0-beta.17",
    "ember-disable-proxy-controllers": "^0.7.0",
    "ember-export-application-global": "^1.0.2",
    "ember-disable-prototype-extensions": "^1.0.0"
  },
  "keywords": [
    "ember-addon",
    "saucelabs",
    "ember",
    "browser",
    "testing"
  ],
  "dependencies": {
    "rsvp": "^3.0.17",
    "sauce-connect-launcher": "^0.11.1",
    "saucie": "^0.1.1"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": [
      "ember-cli-qunit",
      "ember-cli-mocha"
    ]
  }
}
