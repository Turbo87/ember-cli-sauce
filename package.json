{
  "name": "ember-cli-sauce",
  "version": "0.0.3",
  "description": "Cross browser testing for your ember-cli app using SauceLabs.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "start": "ember server",
    "build": "ember build",
    "test": "ember test"
  },
  "bin": "./bin/cli.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/johanneswuerbach/ember-cli-sauce.git"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "Johannes WÃ¼rbach <johannes.wuerbach@googlemail.com>",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.0.0",
    "broccoli-ember-hbs-template-compiler": "^1.6.1",
    "ember-cli": "0.1.5",
    "ember-cli-content-security-policy": "0.3.0",
    "ember-cli-dependency-checker": "0.0.7",
    "ember-cli-esnext": "0.1.1",
    "ember-cli-ic-ajax": "0.1.1",
    "ember-cli-inject-live-reload": "^1.3.0",
    "ember-cli-qunit": "0.1.2",
    "ember-data": "1.0.0-beta.12",
    "ember-export-application-global": "^1.0.0",
    "express": "^4.8.5",
    "glob": "^4.0.5"
  },
  "keywords": [
    "ember-addon",
    "saucelabs",
    "ember",
    "browser",
    "testing"
  ],
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": [
      "ember-cli-qunit"
    ]
  },
  "dependencies": {
    "saucie": "johanneswuerbach/sauce-js-tests-integration#fail-unfinished-runs"
  }
}
