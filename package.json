{
  "name": "ember-cli-sauce",
  "version": "1.4.1",
  "description": "Cross browser testing for your ember-cli app using SauceLabs.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "ember test",
    "test-sauce": "ember sauce --add-to-ci && ember sauce -b 'internet explorer' -p 'Windows 8.1' --protocol=tap --add-to-ci && ember test"
  },
  "bin": "./bin/cli.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/johanneswuerbach/ember-cli-sauce.git"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "Johannes WÃ¼rbach <johannes.wuerbach@googlemail.com>",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.1.2",
    "ember-cli": "1.13.8",
    "ember-cli-app-version": "1.0.0",
    "ember-cli-content-security-policy": "0.4.0",
    "ember-cli-dependency-checker": "^1.0.1",
    "ember-cli-htmlbars": "0.7.9",
    "ember-cli-htmlbars-inline-precompile": "^0.2.0",
    "ember-cli-ic-ajax": "0.2.1",
    "ember-cli-inject-live-reload": "^1.3.1",
    "ember-cli-qunit": "^1.0.0",
    "ember-cli-release": "0.2.3",
    "ember-cli-sri": "^1.0.3",
    "ember-cli-uglify": "^1.2.0",
    "ember-data": "1.13.8",
    "ember-disable-proxy-controllers": "^1.0.0",
    "ember-export-application-global": "^1.0.3",
    "ember-disable-prototype-extensions": "^1.0.0"
  },
  "keywords": [
    "ember-addon",
    "saucelabs",
    "ember",
    "browser",
    "testing"
  ],
  "dependencies": {
    "saucie": "^1.0.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": [
      "ember-cli-qunit",
      "ember-cli-mocha"
    ]
  }
}
